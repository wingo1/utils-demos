<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="355.0" prefWidth="524.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <AnchorPane>
         <children>
            <HBox spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="26.0">
               <children>
                  <Button defaultButton="true" mnemonicParsing="false" text="完成" />
                  <Button fx:id="cancel" cancelButton="true" mnemonicParsing="false" text="取消" />
               </children>
            </HBox>
         </children>
      </AnchorPane>
   </bottom>
   <center>
      <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="name" prefWidth="222.0" text="name" />
          <TableColumn fx:id="age" minWidth="0.0" prefWidth="81.0" text="age" />
            <TableColumn fx:id="birthDate" prefWidth="167.0" text="生日" />
        </columns>
         <BorderPane.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </TableView>
   </center>
</BorderPane>
